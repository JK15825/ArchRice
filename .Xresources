!` rofi
rofi.font:    Helvetica Neue 25
rofi.width:   1000
rofi.columns: 1
rofi.opacity: 75
! rofi.padding: 500
rofi.lines:   5
! NOT THE SAME AS PADDING
! rofi.fullscreen: true

rofi.color-enabled: true
rofi.color-normal: #e6232629, #ffffff, #e6232629, #e63d4247, #ffffff
rofi.color-urgent: #e6232629, #ffffff, #e6232629, #e63d4247, #ffffff
rofi.color-active: #e6232629, #ffffff, #e6232629, #e63d4247, #ffffff
rofi.color-window: #e6232629, #ffffff, #e6232629


rofi.fuzzy: true
rofi.hide-scrollbar: true
rofi.auto-select:false
rofi.separator-style: solid
rofi.bw: 0
rofi.line-margin: 0



! XTerm
XTerm*locale: true
XTerm*foreground: white
XTerm*background: #232629 
XTerm*faceName: Helvetica Neue 
XTerm*selectToClipboard: true
